<?xml version="1.0" encoding="ISO-8859-1"?>
<ReferenceListTable>
    <recordDataXpath>
        ReferenceList/Reference
    </recordDataXpath>
    <formfield>
        <label>SmallImage</label>
        <type>Image</type>
        <id></id>
        <valueXpath>SmallImageUrl</valueXpath>
        <methods>
        	<setHeight>75</setHeight>
        	<setWidth>49</setWidth>
        </methods>
    </formfield>
    <formfield>
        <label>Title</label>
        <type>DisplayOnlyTruncated</type>
        <id>title</id>
        <valueXpath>Title</valueXpath>
        <methods>
        	<setTruncateLength>200</setTruncateLength>
        </methods>
    </formfield>
    <formfield>
        <label>Reference</label>
        <type>DisplayReference</type>
        <id>reference</id>
        <valueXpath></valueXpath>
        <authorsDestination>ReferenceListModal/</authorsDestination>
    </formfield>
    <formfield>
        <label>Folksonomies</label>
        <type>Taxonomy</type>
        <id>referenceTaxonomies</id>
        <valueXpath>TaxonomyList</valueXpath>
        <methods>
        	<setDestination>ReferenceListModal/</setDestination>
        </methods>
    </formfield>
    <formfield>
        <label>ReferenceCount</label>
        <type>MemexPlexObjectCount</type>
        <id></id>
        <valueXpath>ReferenceCount</valueXpath>
        <methods>
            <setObjectType>reference</setObjectType>
        </methods>
    </formfield>
    <formfield>
        <label>MemeCount</label>
        <type>MemexPlexObjectCount</type>
        <id></id>
        <valueXpath>MemeCount</valueXpath>
        <methods>
            <setObjectType>meme</setObjectType>
        </methods>
    </formfield>
</ReferenceListTable>
