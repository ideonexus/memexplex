<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
    <title>Sign Up</title>
	<menu>none</menu>
	<security>none</security>

    <forms>
        <form>
            <id>reportFilter</id>
            <formfield>
                <label></label>
                <type>Hidden</type>
                <id>id</id>
            </formfield>
        </form>
        <form>
            <id>frmSignUp</id>
            <formfield>
                <type>ButtonSubmit</type>
                <id>btnSave</id>
                <methods>
                    <setOnClickJavaScript>FormValidation('frmSignUp');</setOnClickJavaScript>
                </methods>
            </formfield>
        </form>
		<SignUpBlock>
		    <formfield>
		        <label>Email</label>
		        <type>Text</type>
		        <id>email</id>
		        <valueXpath></valueXpath>
		        <methods>
		        	<setSize>40</setSize>
		        	<setMaxlength>256</setMaxlength>
		        </methods>
		    </formfield>
		    <formfield>
		        <label>Display Name</label>
		        <type>Text</type>
		        <id>displayname</id>
		        <valueXpath></valueXpath>
		        <methods>
		        	<setSize>40</setSize>
		        	<setMaxlength>256</setMaxlength>
		        </methods>
		    </formfield>
		    <formfield>
		        <label>Password</label>
		        <type>Password</type>
		        <id>password</id>
		        <valueXpath></valueXpath>
		        <methods>
		        	<setSize>40</setSize>
		        	<setMaxlength>40</setMaxlength>
		        	<setFormAction>FormValidation('frmSignUp');</setFormAction>
		        </methods>
		    </formfield>
		    <formfield>
		        <label>Confirm Password</label>
		        <type>Password</type>
		        <id>confirmpassword</id>
		        <valueXpath></valueXpath>
		        <methods>
		        	<setSize>40</setSize>
		        	<setMaxlength>40</setMaxlength>
		        	<setFormAction>FormValidation('frmSignUp');</setFormAction>
		        </methods>
		    </formfield>
		</SignUpBlock>
    </forms>

</root>
