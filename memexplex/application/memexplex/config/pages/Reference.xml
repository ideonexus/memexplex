<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
    <title>Reference</title>
	<menu>none</menu>
    <javascript>
    	<script>Slide</script>
    </javascript>

    <forms>
        <form>
            <id>reportFilter</id>
            <formfield>
                <label></label>
                <type>HiddenId</type>
                <id>id</id>
            </formfield>
        </form>
        <form>
            <id>frmEditReference</id>
            <formfield>
                <type>ButtonSave</type>
                <id>btnSave</id>
                <methods>
                    <setOnClickJavaScript>FormValidation('frmEditReference');</setOnClickJavaScript>
                </methods>
            </formfield>
            <formfield>
                <type>ConditionalButtonDelete</type>
                <id>btnDelete</id>
                <methods>
                	<setForm>frmEditReference</setForm>
                    <setConfirmation>Reference</setConfirmation>
                </methods>
            </formfield>
        </form>
		<ReferenceTypeBlock>
			<config>ReferenceTypeBlock</config>
		</ReferenceTypeBlock>
		<AuthorsTable>
			<config>AuthorsTable</config>
		</AuthorsTable>
		<ReferenceBlock>
			<config>ReferenceBlock</config>
		</ReferenceBlock>
        <MemeListTable>
			<config>MemeListTable</config>
        </MemeListTable>
    </forms>

</root>
