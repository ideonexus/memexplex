<?xml version="1.0" encoding="ISO-8859-1"?>
<root>
    <title>Reset Password</title>
	<menu>none</menu>
	<security>none</security>

    <forms>
        <form>
            <id>reportFilter</id>
            <formfield>
                <label></label>
                <type>Hidden</type>
                <id>id</id>
            </formfield>
        </form>
        <form>
            <id>frmResetPassword</id>
            <formfield>
                <type>ButtonSubmit</type>
                <id>btnSave</id>
                <methods>
                    <setOnClickJavaScript>FormValidation('frmResetPassword');</setOnClickJavaScript>
                </methods>
            </formfield>
        </form>
        <ResetPasswordBlock>
		    <formfield>
		        <label>Email</label>
		        <type>Text</type>
		        <id>email</id>
		        <valueXpath></valueXpath>
		        <methods>
		        	<setSize>40</setSize>
		        	<setMaxlength>256</setMaxlength>
		        </methods>
		    </formfield>
        </ResetPasswordBlock>
    </forms>

</root>
